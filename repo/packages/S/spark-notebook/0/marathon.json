{
  "id": "spark-notebook",
  "cpus": 4,
  "mem": 4000.0,
  "instances": 1,
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "andypetrella/spark-notebook:0.6.0-scala-2.10.4-spark-1.4.0-hadoop-1.0.4",
      "network": "HOST"
    }
  },
  "healthChecks": [
    {
      "protocol": "HTTP",
      "portIndex": 0,
      "path": "/",
      "gracePeriodSeconds": 5,
      "intervalSeconds": 20,
      "maxConsecutiveFailures": 3
    }
  ]
}
