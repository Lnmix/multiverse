{
  "id": "spark-notebook",
  "cpus": 2,
  "mem": 4000.0,
  "instances": 1,
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "andypetrella/spark-notebook:0.6.0-scala-2.10.4-spark-1.4.1-hadoop-2.4.0-with-hive-with-parquet",
      "network": "HOST"
    }
  },
  "acceptedResourceRoles": ["slave_public"],
  "healthChecks": [
    {
      "protocol": "HTTP",
      "portIndex": 0,
      "path": "/",
      "gracePeriodSeconds": 5,
      "intervalSeconds": 20,
      "maxConsecutiveFailures": 3
    }
  ],
  "ports": [ 0 ]
}
